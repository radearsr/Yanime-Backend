{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "linebreak-style": ["error", "windows"],
        "no-console": ["off"],
        "import/extensions": ["error", { "js": "always", "json": "never" } ]
    }
}
